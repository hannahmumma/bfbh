{
    "name": "bfbh-theme",
    "version": "1.0.0",
    "description": "A marketing site for Brian Frazer's Beach House",
    "main": "index.js",
    "keywords": [],
    "author": "Hannah Mumma",
    "license": "MIT",
    "scripts": {
        "test:a11y": "node tests/pa11y.js",
        "test:scripts": "jest",
        "lint:styles": "stylelint './assets/dev/css/**/*.css' --fix || echo \"lint failed for some file(s).\"",
        "lint:scripts": "eslint './assets/dev/js/**/*.js' --fix || echo \"lint failed for some file(s).\"",
        "lint:php": "./vendor/bin/phpcs --standard=./.phpcs.xml ./src || echo \"lint failed for some file(s).\"",
        "lint:phpfix": "./vendor/bin/phpcbf --standard=./.phpcs.xml ./src || echo \"lint failed for some file(s).\"",
        "pre-commit": "lint-staged",
        "prod": "NODE_ENV=production webpack --config webpack.prod.js",
        "dev": "NODE_ENV=development webpack serve --config webpack.dev.js"
    },
    "lint-staged": {
        "./assets/src/styles/**/*.css": [
            "stylelint --fix"
        ],
        "./assets/src/js/**/*.js": [
            "eslint --fix"
        ]
    },
    "dependencies": {
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "@tailwindcss/line-clamp": "^0.3.0",
        "@wordpress/block-editor": "^8.1.1",
        "@wordpress/blocks": "^11.2.1",
        "@wordpress/components": "^19.4.0",
        "@wordpress/compose": "^5.1.1",
        "@wordpress/element": "^4.1.1",
        "@wordpress/hooks": "^3.3.1",
        "autoprefixer": "^10.4.2",
        "classnames": "^2.3.1",
        "css-minimizer-webpack-plugin": "^3.4.1",
        "cssnano": "^5.0.17",
        "mini-css-extract-plugin": "^2.5.3",
        "node-wp-i18n": "^1.2.6",
        "postcss": "^8.4.6",
        "postcss-import": "^14.0.2",
        "postcss-loader": "^6.2.1",
        "postcss-preset-env": "^7.1.0",
        "postcss-sorting": "^7.0.1",
        "svg-sprite-loader": "^6.0.11",
        "swiper": "^8.0.6",
        "tailwindcss": "^3.0.23",
        "terser-webpack-plugin": "^5.3.1",
        "webpack": "^5.69.1",
        "webpack-merge": "^5.8.0"
    },
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@babel/eslint-parser": "^7.17.0",
        "@babel/plugin-proposal-class-properties": "^7.16.7",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
        "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
        "@babel/plugin-proposal-optional-chaining": "^7.16.7",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.3",
        "babel-register": "^6.26.0",
        "copy-webpack-plugin": "^10.2.4",
        "css-loader": "^6.6.0",
        "eslint": "^8.9.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "lint-staged": "^12.3.4",
        "pa11y": "^6.1.1",
        "path": "^0.12.7",
        "prettier": "^2.5.1",
        "stylelint": "^14.5.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-prettier": "^2.0.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4"
    }
}
